{
	"framework": "sveltekit",
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin-when-cross-origin"
				}
			]
		},
		{
			"source": "/static/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/home",
			"destination": "/",
			"permanent": true
		},
		{
			"source": "/(.*)",
			"destination": "https://modernbrands.co/$1",
			"permanent": true,
			"has": [
				{
					"type": "host",
					"value": "www.modernbrands.co"
				}
			]
		}
	]
}
