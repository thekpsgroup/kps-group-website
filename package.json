{
  "name": "kps-group-modern-suite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:info": "echo 'Use npm run build for production builds. Vercel adapter requires proper permissions on Windows.'",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:full": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "setup:env": "powershell -ExecutionPolicy Bypass -File scripts/setup-env.ps1",
    "env:add": "vercel env add",
    "env:ls": "vercel env ls",
    "env:rm": "vercel env rm",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path ./lighthouse-report.html",
    "security": "npm audit && npx snyk test",
    "performance": "npm run build && lighthouse http://localhost:4173 --only-categories=performance --output json",
    "accessibility": "npm run build && lighthouse http://localhost:4173 --only-categories=accessibility --output json",
    "seo": "npm run build && lighthouse http://localhost:4173 --only-categories=seo --output json",
    "type-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
    "clean": "rm -rf .svelte-kit build dist node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "optimize": "npm run build && npm run analyze",
    "audit:all": "npm audit && npm run security && npm run accessibility",
    "preview:build": "npm run build && npm run preview",
    "dev:analyze": "npm run dev & npm run analyze",
    "build:node": "NODE_ENV=production vite build",
    "build:static": "NODE_ENV=production vite build",
    "start": "node build",
    "check:all": "npm run lint && npm run type-check && npm run test",
    "fix:all": "npm run format && npm run lint -- --fix",
    "update:deps": "npm update && npm audit fix",
    "clean:cache": "rm -rf .svelte-kit node_modules/.vite .vite",
    "validate": "npm run check:all && npm run build",
    "sitemap": "curl http://localhost:5173/sitemap.xml",
    "robots": "curl http://localhost:5173/robots.txt",
    "seo:check": "npm run sitemap && npm run robots",
    "monitor": "npm run dev & npm run lighthouse",
    "deploy:staging": "vercel --env staging",
    "deploy:production": "vercel --prod",
    "health:check": "curl -f http://localhost:5173/ || exit 1",
    "ui:audit": "node scripts/audit-tailwind-classes.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@sentry/svelte": "^10.5.0",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/adapter-vercel": "^5.4.0",
    "@sveltejs/kit": "^2.34.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/google.analytics": "^0.0.46",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "helmet": "^8.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^16.1.5",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.2.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-i18n": "^4.0.1",
    "svelte-sitemap": "^2.7.1",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vite-plugin-bundle-analyzer": "^0.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-minifier": "^1.0.5",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "stripe": "^18.4.0"
  }
}
